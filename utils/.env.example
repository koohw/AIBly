# OpenAI API 키 (필수)
OPENAI_API_KEY=your_openai_api_key_here

# 사용할 모델 (선택, 기본값: gpt-4.1)
MODEL=gpt-4.1

# 응답 다양성 설정 (0.0~1.0)
# 기본값: 1.0
TEMPERATURE=1.0

# 최대 토큰 수
# 기본값: 500
MAX_TOKENS=500

# GitLab 설정
GITLAB_URL=https://lab.ssafy.com
GITLAB_TOKEN=your_gitlab_personal_access_token
GITLAB_PROJECT_ID=your_project_id 